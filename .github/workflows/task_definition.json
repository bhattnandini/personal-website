{
  "family": "deploy-frontend",
  "containerDefinitions": [
    {
      "name": "your-container-name",
      "image": "public.ecr.aws/portfolio-website/portfolio-website:latest",
      "memory": 512,
      "cpu": 256,
      "essential": true,
      "portMappings": [
        {
          "containerPort": 80,
          "hostPort": 80,
          "protocol": "tcp"
        }
      ]
    }
  ]
}